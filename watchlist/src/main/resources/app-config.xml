<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
        <bean id="watchlistRepository" class="com.openClassRoom.watchlist.repository.WatchlistRepository"></bean>
        <bean id="movieRatingService" class="com.openClassRoom.watchlist.service.MovieRatingService"></bean>
        <bean id="watchlistService" class="com.openClassRoom.watchlist.service.WatchlistService">
        	<constructor-arg index="0" ref="watchlistRepository"></constructor-arg>
        	<constructor-arg index="1" ref="movieRatingService"></constructor-arg>
        </bean>
        
 </beans>